@model IEnumerable<ElLib.Common.Entities.Publishing>

@foreach (var publishing in Model)
{
    <tr class="changes">
        <td style="width: 30%;">
            @publishing.Name
        </td>
        <td style="width: 50%;">
            @publishing.Address.Country @publishing.Address.City @publishing.Address.Street @publishing.Address.Home
        </td>
        <td class="td-pad" style="width: 50px">
            <a class="btn-floating btn-sm primary-color" href="@Url.Action("Info", "Publishing", new {id = publishing.Id})"><i class="fa fa-edit"></i></a>
        </td>
        <td class="td-pad" style="width: 50px">
            <a class="btn-floating btn-sm primary-color" href="@Url.Action("Edit", "Publishing", new {id = publishing.Id})"><i class="fa fa-edit"></i></a>
        </td>
        <td class="td-pad" style="width: 50px">
            <a class="btn-floating btn-sm danger-color" href="@Url.Action("Delete", "Publishing", new {id = publishing.Id})"><i class="fa fa-close"></i></a>
        </td>
    </tr>
}