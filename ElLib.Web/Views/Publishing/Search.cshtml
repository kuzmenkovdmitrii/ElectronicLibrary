@model IEnumerable<ElLib.Common.Entities.Publishing>

@foreach (var publishing in Model)
{
    <tr class="changes">
        <td style="width: 30%;">
            @publishing.Name
        </td>
        <td style="width: 50%;">
            @publishing.Address.Country @publishing.Address.City @publishing.Address.Street @publishing.Address.Home
        </td>
        <td class="td-pad" style="width: 50px">
            <a class="btn-floating btn-sm info-color float-right" title="Подробнее" href="@Url.Action("Info", "Publishing", new {id = publishing.Id})"><i class="fa fa fa-info"></i></a>
        </td>
        @if (User.IsInRole("Admin") || User.IsInRole("Editor"))
        {
            <td class="td-pad" style="width: 50px">
                <a class="btn-floating btn-sm primary-color float-right" title="Редактировать" href="@Url.Action("Edit", "Publishing", new {id = publishing.Id})"><i class="fa fa-edit"></i></a>
            </td>
            <td class="td-pad" style="width: 50px">
                <a class="btn-floating btn-sm danger-color float-right" title="Удалить" href="@Url.Action("Delete", "Publishing", new {id = publishing.Id})"><i class="fa fa-close"></i></a>
            </td>
        }
    </tr>
}